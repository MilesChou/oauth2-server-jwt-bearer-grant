{
    "name": "mileschout/oauth2-server-jwt-bearer-grant",
    "description": "Implement RFC 7523 on league/oauth2-server.",
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Chou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "web-token/jwt-signature": "^1.2",
        "web-token/jwt-key-mgmt": "^1.2"
    },
    "require-dev": {
        "ext-gmp": "*",
        "league/oauth2-server": "^6.0",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.3",
        "zendframework/zend-diactoros": "^1.8",
        "psy/psysh": "^0.9.8"
    },
    "suggest": {
        "ext-gmp": "Required to use Elliptic Curves based algorithms.",
        "mdanter/ecc": "Required to use Elliptic Curves based algorithms."
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
